
<p id="notice"><%= notice %></p>

<h1>Temarios</h1>

<table border="1">
  <col width="150">
  <col width="160">
  <col width="150">

  <thead>
    <tr>
      <th>Nombre del temario</th>
        <th>Descripción</th>


      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @temario.each do |t| %>
      <tr>
        <td><%= t.nombretemario %></td>
          <td><%= t.descripcion %></td>


        <td><%= link_to 'Show', controller: "temarios", action: "show", id: t.id  %></td>
        <td><%= link_to 'Editar', edit_admin_temario_path(t) %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<%= will_paginate @temario,  :previous_label => 'Atrás', :next_label => 'Adelante'%>
<br>

<%= link_to 'Nuevo temario', new_admin_temario_path %>
